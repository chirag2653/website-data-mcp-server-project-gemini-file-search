{
  "name": "website-data-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for website data ingestion and semantic querying using Gemini File Search",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest"
  },
  "keywords": [
    "mcp",
    "gemini",
    "firecrawl",
    "supabase",
    "semantic-search",
    "rag"
